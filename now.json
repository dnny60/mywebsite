{
    "version": 2,
    "alias": "dannywong.now.sh",
    "builds": [
        { "src": "app.js", "use": "@now/node-server" },
        { "src": "*.html", "use": "@now/static" }
    ],
    "routes": [
        {
          "src": "/",
          "dest": "app.js",
          "methods": ["GET"]
        }
      ]
}